{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/images/*.{png,jpg,gif}", "/css/*", "/js/*", "/assets/*"]
  },
  "routes": [
    {
      "route": "/*",
      "serve": "/index.html",
      "statusCode": 200
    }
  ],
  "mimeTypes": {
    ".json": "text/json",
    ".js": "text/javascript",
    ".css": "text/css",
    ".html": "text/html",
    ".svg": "image/svg+xml"
  },
  "globalHeaders": {
    "cache-control": "no-cache",
    "Content-Security-Policy": "default-src 'self' 'unsafe-inline' 'unsafe-eval' https://tech0-gen-8-step4-eyesmile-back.azurewebsites.net; img-src 'self' data: https:; style-src 'self' 'unsafe-inline';",
    "X-Frame-Options": "DENY",
    "X-Content-Type-Options": "nosniff"
  }
} 